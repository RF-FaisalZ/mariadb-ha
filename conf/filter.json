{
    "rules": [
        {
            "replace": {
                "column": "nric",
                "match": "^.{0,5}"
            },
            "with": {
                "fill": "*"
            },
                "exempted": [ "maxuser", "root" ]
        },
        {
                "replace": {
                        "column": "name"
                },
                "obfuscate": {
                        "column": "name"
                },
                "exempted": [ "maxuser", "root" ]
        },
        {
            "replace": {
                "column": "phone",
                "match": ".{0,3}$"
            },
            "with": {
                "fill": "*"
            },
            "exempted": [ "maxuser", "root" ]
        }
    ]
}
